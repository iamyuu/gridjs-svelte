    "svelte-check": "^1.1.26",
    "svelte-jester": "^1.3.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.6.1",
    "ts-jest": "^26.4.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
